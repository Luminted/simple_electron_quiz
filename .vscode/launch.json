{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "chrome",
      "request": "launch",
      "webRoot": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "runtimeArgs": [
          "${workspaceRoot}",
          "--enable-logging",
          "--remote-debugging-port=9222"
       ],
       "sourceMaps": false,
       "cwd": "${workspaceRoot}",
       "console": "internalConsole"
  }
  ],
  "compounds": []
}